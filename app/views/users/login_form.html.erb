<h1>User Login</h1>
<%= form_with model: @user, local: true do |form| %>
  <%= form.label :email, "Email:" %>
  <%= form.text_field :email, Placeholder: "email" %>
  <%= form.label :password, "Password:" %>
  <%= form.password_field :password, Placeholder: "password" %>
  <%= form.submit "Login" %>
<% end %>